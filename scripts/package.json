{
  "name": "ragestate-db-migration",
  "version": "1.0.0",
  "description": "Migration scripts for RAGESTATE database",
  "type": "module",
  "scripts": {
    "migrate:dry": "node migrateUsersToFirestore.js",
    "migrate:live": "node migrateUsersToFirestore.js --live",
    "sync:photos:dry": "node syncProfilePictures.js",
    "sync:photos:live": "node syncProfilePictures.js --live",
    "sync:photos:overwrite": "node syncProfilePictures.js --live --overwrite",
    "backfill:usernames:dry": "node backfillUsernames.js",
    "backfill:usernames:live": "node backfillUsernames.js --live",
    "manual:create-ticket": "node manualCreateTicket.js",
    "manual:create-ticket:fulfillment": "node manualCreateTicket.js --createFulfillment"
  },
  "dependencies": {
    "firebase-admin": "^11.10.1"
  }
}
